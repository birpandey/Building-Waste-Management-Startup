<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.waste.fragments.Home"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_wasteItems3"
            app:destination="@id/wasteItems" />
    </fragment>

    <fragment
        android:id="@+id/nav_address"
        android:name="com.example.waste.fragments.PickUpAddress"
        android:label="@string/pick_up_address"
        tools:layout="@layout/fragment_pick__up__address" />
    <fragment
        android:id="@+id/nav_orders"
        android:name="com.example.waste.fragments.Orders"
        android:label="@string/orders"
        tools:layout="@layout/fragment_orders" />
    <fragment
        android:id="@+id/nav_support"
        android:name="com.example.waste.fragments.Support"
        android:label="@string/customer_support"
        tools:layout="@layout/fragment_support" />
    <fragment
        android:id="@+id/nav_about"
        android:name="com.example.waste.fragments.About"
        android:label="@string/about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/nav_rating"
        android:name="com.example.waste.fragments.Feedback"
        android:label="@string/feedback"
        tools:layout="@layout/fragment_feedback" />
    <fragment
        android:id="@+id/wasteItems"
        android:name="com.example.waste.fragments.WasteItems"
        android:label="fragment_waste_items"
        tools:layout="@layout/fragment_waste_items" />

</navigation>